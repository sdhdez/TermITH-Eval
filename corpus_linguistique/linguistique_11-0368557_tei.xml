            <keywords scheme="inist-francis" xml:lang="en">
               <term type="controlled" xml:id="Sociohistoricalapproach">Sociohistorical approach</term>
               <term type="controlled" xml:id="Speechturn">Speech turn</term>
               <term type="controlled" xml:id="Pragmatics">Pragmatics</term>
               <term type="controlled" xml:id="Discoursestrategy">Discourse strategy</term>
               <term type="controlled" xml:id="Process">Process</term>
               <term type="controlled" xml:id="Corpusanalysis">Corpus analysis</term>
