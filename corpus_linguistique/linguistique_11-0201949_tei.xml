               <term type="controlled" xml:id="Semasiology">Semasiology</term>
               <term type="controlled" xml:id="Lexicology">Lexicology</term>
               <term type="controlled" xml:id="Semantics">Semantics</term>
               <term type="controlled" xml:id="Onomasiology">Onomasiology</term>
