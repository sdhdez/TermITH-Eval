               <term type="controlled" xml:id="Speechcommunity">Speech community</term>
               <term type="controlled" xml:id="Sociolinguistics">Sociolinguistics</term>
               <term type="controlled" xml:id="Didactics">Didactics</term>
               <term type="controlled" xml:id="Exolingualcommunication">Exolingual communication</term>
