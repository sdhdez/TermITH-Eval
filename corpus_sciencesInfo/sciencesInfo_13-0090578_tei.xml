            <keywords scheme="inist-pascal" xml:lang="en">
               <term type="controlled" xml:id="Internet">Internet</term>
               <term type="controlled" xml:id="Participation">Participation</term>
               <term type="controlled" xml:id="Worldwideweb">World wide web</term>
