            <keywords scheme="inist-pascal" xml:lang="en">
               <term type="uncontrolled" xml:id="Folksonomy">Folksonomy</term>
               <term type="controlled" xml:id="Virtualcommunity">Virtual community</term>
               <term type="controlled" xml:id="Socialnetwork">Social network</term>
               <term type="controlled" xml:id="Classification">Classification</term>
               <term type="controlled" xml:id="Indexing">Indexing</term>
               <term type="controlled" xml:id="Knowledgeorganization">Knowledge organization</term>
               <term type="controlled" xml:id="Informationmanagement">Information management</term>
